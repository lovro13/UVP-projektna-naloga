    <script>
        const addGoogleOneTap = () => {
            const scriptElement = document.createElement( 'script' );
            scriptElement.async = true;
            scriptElement.defer = true;
            scriptElement.setAttribute('src', 'https://accounts.google.com/gsi/client');
            document.body.appendChild(scriptElement);
        }

        var consentCounter = 0;
        const consentInterval = setInterval(function () {
            consentCounter += 1;

            if (consentCounter >= 60) {
                addGoogleOneTap();
                clearInterval(consentInterval);
            }

            if (typeof window.__tcfapi !== 'undefined') {
                clearInterval(consentInterval);

                window.__tcfapi('addEventListener', 2, function(tcData) {
                    if (tcData.eventStatus === 'useractioncomplete' || tcData.eventStatus === 'tcloaded') {
                        addGoogleOneTap();
                    }
                } );
            }
        }, 1000);

        const consentBlockedInterval = setInterval(function () {
            if (typeof window.__tcfapi === 'undefined') {
                clearInterval(consentInterval);
                addGoogleOneTap();
            }

            clearInterval(consentBlockedInterval);
        }, 3000);

        const oneTapLogout = () => {
            if (typeof window.google !== 'undefined') {
                google.accounts.id.disableAutoSelect();
            }
        }
    </script>
